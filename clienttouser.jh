
application {
  config {
    baseName iluteenusteApp,
    applicationType monolith,
    packageName ee.shtlx.iluteenusteapp,
    authenticationType jwt,
    prodDatabaseType sql,
    clientFramework angularX
  }
  entities *
}

entity Client {
	isShopOwner Boolean required /* Shows, if client is a shop owner **/,
    tel String min(0) max(15) required /* Client telephone number **/
}

relationship OneToOne {
  Client{user} to User
}
    